    {{ <xref ref="exer_indexpositivereals"></xref>.}}
    \item
    <ul>
      {{(a)}}
      \item <m>\left\{ x \in \R \left| -100 \leq x \leq 100 \right. \right\}</m>
    </ul>
  </ul>
  <ul>
    \item
    <ul>
      {{(b)}}
      \item <m>\left\{ x \in \R \left| -1 \leq x \leq 1 \right. \right\}</m>
    </ul>
